<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OrEnterBy">

    <RelativeLayout android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/or_enter_by_background"
        >

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/header_or_enter_by"

            android:layout_width="328dp"
            android:layout_height="43dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="61dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/rectangle_1"
            android:layout_width="96dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="122dp"
            android:background="@drawable/small_rectangle"
            android:elevation="25dp"
            />

        <View
            android:id="@+id/rectangle_2"
            android:layout_width="96dp"
            android:layout_height="56dp"
            android:layout_toEndOf="@id/rectangle_1"
            android:layout_marginStart="16dp"
            android:layout_marginTop="122dp"
            android:background="@drawable/small_rectangle"
            android:elevation="25dp"
            />

        <View
            android:id="@+id/rectangle_3"
            android:layout_width="96dp"
            android:layout_height="56dp"
            android:layout_toEndOf="@id/rectangle_2"
            android:layout_marginStart="16dp"
            android:layout_marginTop="122dp"

            android:background="@drawable/small_rectangle"
            android:elevation="25dp" />


    </RelativeLayout>

</FrameLayout>