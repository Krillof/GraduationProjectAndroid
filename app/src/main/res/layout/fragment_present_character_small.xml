<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.for_main_page.dos.PresentCharacterSmall">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/header"

        android:layout_width="328dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <View
        android:id="@+id/character_rectangle"
        android:layout_width="156dp"
        android:layout_height="156dp"
        android:layout_marginTop="61dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/tmp_square_present_character_small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/picture_character"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/character_rectangle"
        app:layout_constraintTop_toTopOf="@+id/character_rectangle"
        app:layout_constraintEnd_toEndOf="@+id/character_rectangle"
        app:layout_constraintBottom_toBottomOf="@id/character_rectangle"
        />

    <ImageView
        android:id="@+id/picture_hair"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/character_rectangle"
        app:layout_constraintTop_toTopOf="@+id/character_rectangle"
        app:layout_constraintEnd_toEndOf="@+id/character_rectangle"
        app:layout_constraintBottom_toBottomOf="@id/character_rectangle"
        />

    <ImageView
        android:id="@+id/picture_shirt"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/character_rectangle"
        app:layout_constraintTop_toTopOf="@+id/character_rectangle"
        app:layout_constraintEnd_toEndOf="@+id/character_rectangle"
        app:layout_constraintBottom_toBottomOf="@id/character_rectangle"
        />

    <TextView
        android:id="@+id/health_text_header"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="82dp"
        android:layout_marginStart="196dp"

        android:text="@string/health"
        android:textAppearance="@style/small_text_style"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/health_bar_background"

        android:layout_width="156dp"
        android:layout_height="18dp"

        android:layout_marginTop="100dp"
        android:layout_marginStart="188dp"

        android:background="@drawable/health_bar_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/health_bar_circle_1"

        android:layout_width="18dp"
        android:layout_height="18dp"

        android:layout_marginStart="-9dp"


        android:background="@drawable/health_bar_circle"
        app:layout_constraintStart_toStartOf="@+id/health_bar_rect"
        app:layout_constraintTop_toTopOf="@+id/health_bar_rect"
        app:layout_constraintBottom_toBottomOf="@+id/health_bar_rect"
        />

    <View
        android:id="@+id/health_bar_rect"

        android:layout_width="142dp"
        android:layout_height="18dp"

        android:layout_marginTop="100dp"
        android:layout_marginStart="195dp"



        android:background="@drawable/health_bar_rectangle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/health_bar_circle_2"

        android:layout_width="18dp"
        android:layout_height="18dp"

        android:layout_marginStart="-9dp"


        android:background="@drawable/health_bar_circle"
        app:layout_constraintStart_toEndOf="@+id/health_bar_rect"
        app:layout_constraintTop_toTopOf="@+id/health_bar_rect"
        app:layout_constraintBottom_toBottomOf="@+id/health_bar_rect"
        />

    <TextView
        android:id="@+id/health_text"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="101dp"
        android:layout_marginStart="248dp"

        android:textAppearance="@style/bar_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/exp_text_header"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="122dp"
        android:layout_marginStart="196dp"

        android:text="@string/experience"
        android:textAppearance="@style/small_text_style"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/exp_bar_background"

        android:layout_width="156dp"
        android:layout_height="18dp"

        android:layout_marginTop="140dp"
        android:layout_marginStart="188dp"

        android:background="@drawable/exp_bar_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <View
        android:id="@+id/exp_bar_circle_1"

        android:layout_width="18dp"
        android:layout_height="18dp"

        android:layout_marginStart="-9dp"


        android:background="@drawable/exp_bar_circle"
        app:layout_constraintStart_toStartOf="@+id/exp_bar_rect"
        app:layout_constraintTop_toTopOf="@+id/exp_bar_rect"
        app:layout_constraintBottom_toBottomOf="@+id/exp_bar_rect"
        />

    <View
        android:id="@+id/exp_bar_rect"

        android:layout_width="142dp"
        android:layout_height="18dp"

        android:layout_marginTop="140dp"
        android:layout_marginStart="195dp"



        android:background="@drawable/exp_bar_rectangle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:id="@+id/exp_bar_circle_2"

        android:layout_width="18dp"
        android:layout_height="18dp"

        android:layout_marginStart="-9dp"


        android:background="@drawable/exp_bar_circle"
        app:layout_constraintStart_toEndOf="@+id/exp_bar_rect"
        app:layout_constraintTop_toTopOf="@+id/exp_bar_rect"
        app:layout_constraintBottom_toBottomOf="@+id/exp_bar_rect"
        />

    <TextView
        android:id="@+id/exp_text"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="141dp"
        android:layout_marginStart="248dp"

        android:textAppearance="@style/bar_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/level_text"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="184dp"
        android:layout_marginStart="188dp"

        android:text="Уровень 13"
        android:textAppearance="@style/text_style"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <View
        android:layout_width="18dp"
        android:layout_height="13dp"
        android:layout_marginStart="290dp"
        android:layout_marginTop="188dp"

        android:background="@drawable/money"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <TextView
        android:id="@+id/money_text"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="311dp"
        android:layout_marginTop="184dp"

        android:text="129"
        android:textAppearance="@style/text_style"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>